    "@types/node": "^24.7.2",
    "@vitejs/plugin-vue": "^6.0.1",
    "@vue/tsconfig": "^0.8.1",
    "chromatic": "^13.3.0",
    "husky": "^9.1.7",
    "install": "^0.13.0",
    "nx": "21.6.4",
    "storybook": "^9.1.10",
    "typescript": "~5.9.3",
    "vite": "^7.1.7",
    "vue-tsc": "^3.1.0"
  },
  "nx": {}
